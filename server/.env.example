# Server environment variables
# Copy this to .env and fill in your values

# Required for Oura Ring data
OURA_API_KEY=

# Required for AI chat responses
ANTHROPIC_API_KEY=

# Required for text-to-speech
ELEVENLABS_API_KEY=

# Supabase (if using)
SUPABASE_URL=
SUPABASE_ANON_KEY=

# Database (for production on Render)
DATABASE_URL=

# Frontend URL for CORS (set in production)
# Example: https://best-self-app.onrender.com
FRONTEND_URL=

# Port (Render sets this automatically)
PORT=3000
